<!DOCTYPE html>
<html lang="en">

<head>
    <%- include('../partials/head'); %>
</head>

<body class="container">

    <header>
        <%- include('../partials/header'); %>
    </header>

    <main>
        <div class="jumbotron">
            <h3>Users</h3>

            <table>
                <tr>
                    <th>Username</th>
                    <th>Email</th>
                    <!-- <th>Password</th> -->
                </tr>
                <% for(var i=0; i < data.length; i++) { %>
                    <tr>
                        <td>
                            <%= data[i].username %>
                        </td>
                        <td>
                            <%= data[i].email %>
                        </td>
                        <!-- <td><%= //data[i].mobile %></td> -->
                    </tr>
                    <% } %>
            </table>
        </div>
    </main>

    <footer>
        <%- include('../partials/footer'); %>
    </footer>

</body>

</html>